{
  "name": "minutephp/minute",
  "description": "The Minute Framework",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Sanchit Bhatnagar",
      "email": "dev@minutephp.com"
    }
  ],
  "require": {
    "php": ">=7",
    "minutephp/framework": "dev-master",
    "robmorgan/phinx": "^0.6.4",
    "minutephp/site": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Minute\\": "minute/",
      "Test\\": "test/"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "cd public/static && bower -F install",
      "cd public/static && typings install",
      "php vendor/minutephp/cli/bin/minutephp first-run"
    ],
    "post-package-install": [
      "App\\Config\\Plugins::installPlugin"
    ],
    "pre-package-uninstall": [
      "App\\Config\\Plugins::removePlugin"
    ]
  },
  "minimum-stability": "dev",
  "repositories": {
    "0": {
      "packagist": false
    },
    "minutephp/test": {
      "type": "vcs",
      "url": "E:/var/Dropbox/projects/github/minutephp/test"
    },
    "minutephp/test2": {
      "type": "vcs",
      "url": "E:/var/Dropbox/projects/github/minutephp/test2"
    }
  }
}
